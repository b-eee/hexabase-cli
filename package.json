{
  "name": "hexabase-cli",
  "description": "Hexabase CLI",
  "version": "0.0.1",
  "author": "devops@b-eee.com",
  "bin": {
    "hx": "./bin/run"
  },
  "bugs": "https://github.com/b-eee/hexabase-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.2.0",
    "@types/download": "^6.2.4",
    "@types/eventsource": "^1.1.4",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "conf": "^7.1.2",
    "download": "^8.0.0",
    "enquirer": "^2.3.6",
    "eventsource": "^1.0.7",
    "tslib": "^1.14.1",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.39",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/b-eee/hexabase-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "apps": {
        "description": "APPS topic"
      },
      "contexts": {
        "description": "CONTEXTS topic"
      },
      "logs": {
        "description": "LOGS topic"
      },
      "projects": {
        "description": "PROJECTS topic"
      },
      "workspaces": {
        "description": "WORKSPACES topic"
      }
    },
    "bin": "hx",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "b-eee/hexabase-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}